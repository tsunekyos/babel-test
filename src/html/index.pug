doctype html
head
  link(rel="stylesheet" href="./css/style.css")
  meta(name="viewport" content="width=device-width,initial-scale=1.0")
body
  header HEADER
  div.products
    p.product(data-id="a" data-ang="oblique")
    p.product(data-id="b" data-ang="oblique")
    p.product(data-id="c" data-ang="oblique")
    p.product(data-id="d" data-ang="oblique")
    p.product(data-id="e" data-ang="oblique")
    p.product(data-id="f" data-ang="oblique")
      
  div.moreBtn MORE
  
  div.switchBtns
    each val in ["oblique","left","back","right","sole","top","detail","usage"]
      div.switchBtn(data-ang=val)=val
    
  script(src="https://code.jquery.com/jquery-3.3.1.min.js")
  script(src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.3/TweenMax.min.js")
  script(src="./js/bundle.js")
